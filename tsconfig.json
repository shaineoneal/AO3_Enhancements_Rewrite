{
    "compilerOptions": {
        "strict": true,
        "target": "es6",
        "moduleResolution": "bundler",
        "module": "ES6",
        "esModuleInterop": true,
        "sourceMap": false,
        "rootDir": "src",
        "outDir": "dist/js",
        "noEmitOnError": true,
        "noImplicitAny": false,
        "jsx": "react-jsx",
        "typeRoots": [ "node_modules/@types" ]
    },
    "typedocOptions": {
        "entryPoints": ["src/index.ts"],
        "out": "docs",
        
    }
}
